<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <padding android:bottom="8dp" android:left="60dp" android:right="60dp" android:top="8dp"/>
            <corners android:radius="5dp"/>
            <solid android:color="@color/default_app_f_color"/>
            <stroke android:width="1px" android:color="@color/default_app_f_color"/>
        </shape>
    </item>
    <item>
        <shape>
            <padding android:bottom="8dp" android:left="60dp" android:right="60dp" android:top="8dp"/>
            <corners android:radius="5dp"/>
            <solid android:color="@android:color/white"/>
            <stroke android:width="1px" android:color="@color/default_app_f_color"/>
        </shape>
    </item>
</selector>